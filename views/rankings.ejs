<body>

	<label for="url">Enter an https:// URL:</label>
	<button id="highlight-team">Import team</button>
	<button type="submit" id="web-team" form="web-team-form">Get team from web</button>

	<form class="web-team-form" id="web-team-form">
		<input type="url" name="url" id="url" form="web-team-form" placeholder="paste team link here" pattern="https://.*" size="30"
			required>
	</form>

	<select name="category" id="sort">
		<!-- <option value="">--Select a Stat--</option> -->
		<option value="score">Total Score</option>
		<option value="goals">Goals</option>
		<option value="assists">Assists</option>
		<option value="points">Points</option>
		<option value="ppp">PPP</option>
		<option value="sog">SOG</option>
		<option value="hits">Hits</option>
		<option value="pims">Pims</option>
		<option value="blks">Blocks</option>
	</select>

	<div id="key">

	</div>


	<div id="bar-chart-wrapper">

	</div>
	<iframe id="player-card-iframe" style="display: none">
	</iframe>
		<div id="player-card-wrapper">

		</div>
		

	<!-- </iframe> -->
</body>

